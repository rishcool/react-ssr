{
	"name": "React-SSR",
	"version": "1.0.0",
	"description": "",
	"main": "src/server/index.js",
	"scripts": {
		"start": "NODE_ENV='development' && npm run compile & npm run dev-server",
		"compile": "webpack --progress --colors --watch",
		"dev-server": "cross-env NODE_PATH=./src concurrently --kill-others \"nodemon src/server\" ",
		"prod": "npm run prod:deploy && npm run prod:build && npm run prod:server",
		"prod:deploy": "cross-env ./node_modules/.bin/babel src -d build",
		"prod:build": "NODE_ENV=production webpack --config webpack.config.prod.babel.js --progress",
		"prod:server": "NODE_ENV=production cross-env NODE_PATH=./src concurrently --kill-others \"node build/server\" ",
		"analyze": "NODE_ENV='analyze' npm run analyze:dev-build && npm run dev-server",
		"analyze:dev-build": "webpack --progress --colors --watch"
	},
	"keywords": [],
